{
  "entrypoint": "index.html",
  "shell": "src/fsn-app.html",
  "fragments": [
    "src/fsn-vatsim-events-view.html",
    "src/fsn-weather-view.html",
    "src/fsn-oceanic-report-view.html",
    "src/my-view404.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "name": "heroku",
      "bundle": true,
      "js": {"minify": true},
      "css": {"minify": true},
      "html": {"minify": true}
    },
    {
      "name": "github.io",
      "bundle": true,
      "js": {"minify": true},
      "css": {"minify": true},
      "html": {"minify": true},
      "basePath": "fsn"
    }
  ]
}
